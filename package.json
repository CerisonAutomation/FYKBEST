  "scripts": {
    "dev": "next dev --turbopack",
    "dev:turbo": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format . --write",
    "format:check": "biome format . --dry-run",
    "type-check": "tsc --noEmit --incremental",
    "type-check:watch": "tsc --noEmit --incremental --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "analyze": "ANALYZE=true next build",
    "db:generate": "supabase gen types typescript --local > lib/database.types.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "tsx scripts/seed.ts",
    "validate:env": "chmod +x scripts/validate-env.sh && ./scripts/validate-env.sh",
    "setup": "npm run validate:env && npm install",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules .turbo",
    "postinstall": "npx next telemetry disable"
  },
